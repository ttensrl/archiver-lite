{
    "name": "laravel-bricks/archiver-lite",
    "description": "Manage Easy Upload of Media and file.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Stefano Pimpolari",
            "email": "stefano.pimpolari@tten.it"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "spatie/laravel-medialibrary": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelBricks\\ArchiverLite\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelBricks\\ArchiverLite\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelBricks\\ArchiverLite\\ArchiverLiteServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
